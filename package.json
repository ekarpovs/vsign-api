  "keywords": [
    "VSIGN",
    "REST",
    "API"
  ],
  "author": "Evgeny Karpovsky",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.0",
    "method-override": "^3.0.0",
    "mongoose": "^5.5.9",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^7.0.0",
    "@types/method-override": "0.0.31",
    "@types/mongoose": "^5.5.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.2",
    "@types/shelljs": "^0.8.5",
    "fs-extra": "^8.0.1",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}
